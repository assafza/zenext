{
    "name": "ZeNext",
    "description": "Advanced Zendesk ticket manager",
    "version": "1.0",

    "permissions": [
    	"notifications",
        "https://*.zendesk.com/",
        "storage",
        "tabs"
    ],
    "icons": {
    "16": "logo_small.png",
    "48": "logo_small.png",
    "128": "logo_small.png"
    },
    "browser_action": {
        "default_popup": "index.html"
    },
    "manifest_version": 2,
    "background": {
    	"scripts": [
            "axios.min.js",
            "background.js"
        ]
  	}

}
